<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="SSEPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.entities.FormularioFinalizacion</class>
    <class>com.entities.Ciclo</class>
    <class>com.entities.Nota</class>
    <class>com.entities.Usuario</class>
    <class>com.entities.Empleado</class>
    <class>com.entities.TipoUsuario</class>
    <class>com.entities.Estudiante</class>
    <class>com.entities.Sede</class>
    <class>com.entities.HojaControl</class>
    <class>com.entities.Carrera</class>
    <class>com.entities.InscripcionCiclo</class>
    <class>com.entities.HorariosSse</class>
    <class>com.entities.Dia</class>
    <class>com.entities.Institucion</class>
    <class>com.entities.Actividad</class>
    <class>com.entities.Materia</class>
    <class>com.entities.Escuela</class>
    <class>com.entities.HorarioAtencion</class>
    <class>com.entities.TipoCarrera</class>
    <class>com.entities.Solicitud</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/sse"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
